# PostgreSQL Database
POSTGRES_USER=jobportal_user
POSTGRES_PASSWORD=jobportal_password
POSTGRES_DB=jobportal

# Fixed API Keys for Backend
# IMPORTANT: Change these values in production!
# These keys are automatically created/updated when the backend starts
#
# API_KEY_ADMIN: Full admin access (all permissions)
# API_KEY_WEBSCRAPER: Read + write + read_hidden (for scrapers)
# API_KEY_FULLREAD: Read + read_hidden (analytics/reporting)
# API_KEY_FRONTEND: Read only, no hidden access (public UI)
#
API_KEY_ADMIN=admin_key_change_in_production
API_KEY_WEBSCRAPER=webscraper_key_change_in_production
API_KEY_FULLREAD=fullread_key_change_in_production
API_KEY_FRONTEND=frontend_key_change_in_production
